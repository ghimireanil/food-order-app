tory.replace('/orders');\n      // });\n    };\n  };\n\n  const handleChange = (event) => {\n    // listen for changes in card and display error as customer tpes card details\n    setDisabled(event.empty);\n    setError(event.error ? event.error.message : '');\n  };\n\n  return (\n    <div className='payment'>\n      <div className='payment_container'>\n        <h1>\n          Checkout (<Link to='/checkout'>{basket?.length} items</Link>)\n        </h1>\n\n        {/* payment section - delvery address */}\n        <div className='payment_section'>\n          <div className='payment_title'>\n            <h3>Delivery Address</h3>\n          </div>\n          <div className='payment_address'>\n            <p>{user?.email}</p>\n            <p>84th St, Northern Blvd</p>\n            <p>New York, NY, 11372</p>\n          </div>\n        </div>\n\n        {/* payment section - review Items */}\n        <div className='payment_section'>\n          <div className='payment_title'>\n            <h3>Review Items for delivery</h3>\n          </div>\n          <div className='payment_items'>\n            {basket.map((item) => (\n              <CheckoutProduct\n                id={item.id}\n                title={item.title}\n                image={item.image}\n                price={item.price}\n                rating={item.rating}\n              />\n            ))}\n          </div>\n        </div>\n      </div>\n\n      {/* payment section - payment method */}\n      <div className='payment_section'>\n        <div className='payment_title'>\n          <h3>Payment Method</h3>\n        </div>\n        <div className='payment_details'>\n          {/* stripe will go here */}\n          <form onSubmit={handleSubmit}>\n            <CardElement onChange={handleChange} />\n\n            <div className='payment_priceContainer'>\n              <CurrencyFormat\n                renderText={(value) => (\n                  <>\n                    <h3>Order Total: {value}</h3>\n                  </>\n                )}\n                decimalScale={2}\n                value={getBasketTotal(basket)}\n                displayType={'text'}\n                thousandSeperator={true}\n                prefix={'$'}\n              />\n              {/* <button disabled={processing || disabled || succeeded}>\n                <span>{processing ? <p>Processing</p> : 'Buy Now'}</span>\n              </button> */}\n              <Link to='orders'>Buy Now</Link>\n            </div>\n            {/* If there is any error */}\n            {error && <div>{error}</div>}\n          </form>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Payment;\n","/Users/anilghimire/Desktop/food-order-app/src/Food Order App/pages/HomePage/Home.js",[],"/Users/anilghimire/Desktop/food-order-app/src/Food Order App/pages/Resturants/Resturants.js",[],"/Users/anilghimire/Desktop/food-order-app/src/Food Order App/pages/Services/Services.js",[],"/Users/anilghimire/Desktop/food-order-app/src/Food Order App/RestaurantSearch/SearchingRestaurant.js",[],"/Users/anilghimire/Desktop/food-order-app/src/Food Order App/RestaurantDetail/Image.js",["227"],"import React from \"react\";\n\nexport function Image(props) {\n  const { image } = props;\n\n  return (\n    <>\n      <img style={{ width: \"95%\", height: \"450px\" }} src={image} />\n    </>\n  );\n}\n","/Users/anilghimire/Desktop/food-order-app/src/Food Order App/RestaurantDetail/RestaurantDetail.js",["228"],"import React from \"react\";\nimport style from \"./RestaurantDetail.module.css\";\n\nexport function RestaurantDetail(props) {\n  const {\n    name,\n    hours,\n    address,\n    contact,\n    features,\n    review,\n    foodReview,\n    envReview,\n    priceReview,\n    serviceReview,\n    restType,\n  } = props;\n\n  return (\n    <>\n      <div className={style.restro}>\n        <div className={style.text}>\n          <strong>\n            Address: <p> {address}</p>\n            Business Hour: <p> {hours}</p>\n          